{
  "manifest_version": 2,

  "name": "STAY HUMBLE.",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon-16x16.png",
    "default_popup": "popup.html",
    "default_title": "CLICK HERE!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}